{
    "name": "nk/system-auth",
    "description": "A Laravel package for key-based authentication and license verification",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nand Kathiriya",
            "email": "nkbhai242001@gmail.com"
        }
    ],
    "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/routing": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/session": "^9.0|^10.0|^11.0|^12.0"
},
    "autoload": {
        "psr-4": {
            "Nk\\SystemAuth\\": "src/"
        },
        "files": [
             "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nk\\SystemAuth\\SystemAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}